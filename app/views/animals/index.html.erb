動物一覧
<% @animals.each do |animal|%>
<%= link_to animal_path(animal) do %>
<%= attachment_image_tag animal, :image%>
<% end %>
<%= link_to user_path(animal.user.id) do %>
<%= attachment_image_tag animal.user, :profile_image,fallback: 'no-image.png'%>
<% end %>
<%= link_to animal.user.name, user_path(animal.user.id) %>
<%= animal.updated_at.strftime("%Y-%m-%d %H:%M")%>
<%end%>
